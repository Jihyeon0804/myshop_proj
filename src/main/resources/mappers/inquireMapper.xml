<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.myshop.inquire.mapper.InquireMapper">
	<insert id="insertInquire" parameterType="map">
		INSERT INTO `inquire`
		(
			`userId`
			, `productId`
			, `type`
			, `title`
			, `content`
			, `createdAt`
			, `updatedAt`
		)
		VALUES
		(
			#{userId}
			, #{productId}
			, #{type}
			, #{title}
			, #{content}
			, NOW()
			, NOW()
		)
	</insert>
</mapper>