<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.myshop.delivery.mapper.DeliveryMapper">
	<select id="selectDeliveryByOrderId" parameterType="int" resultType="com.myshop.delivery.domain.Delivery">
		SELECT
			
		FROM
			`delivery`
		WHERE
			`orderId` = #{orderId}
	</select>
</mapper>