<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.myshop.admin.user.mapper.UserAdminMapper">
	<select id="selectUserList" resultType="map">
		SELECT
			`id`
			, `loginId`
			, `password`
			, `name`
			, `email`
			, `phoneNumber`
			, `birth`
			, `address`
			, `grade`
			, `joinType`
			, `createdAt`
			, `updatedAt`
		FROM
			`user`
	</select>
</mapper>