<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.myshop.product.mapper.ProductMapper">
	<select id="selectProductList">
		SELECT
			`id`
			, `categoryId`
			, `subclassId`
			, `optionId`
			, `title`
			, `description`
			, `price`
			, `discountPrice`
			, `createdAt`
			, `updatedAt`
		FROM
		 `product`
	</select>
</mapper>